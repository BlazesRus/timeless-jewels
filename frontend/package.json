{
  "name": "frontend",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite dev",
    "dev:svelte4": "node scripts/version-manager.js switchTo4 && vite dev",
    "dev:svelte5": "node scripts/version-manager.js switchTo5 && vite dev",
    "build": "vite build",
    "build:svelte4": "node scripts/version-manager.js switchTo4 && vite build",
    "build:svelte5": "node scripts/version-manager.js switchTo5 && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint . --no-warn-ignored",
    "format": "prettier --write .",
    "test:version": "node -e \"console.log('Svelte version:', require('svelte/package.json').version)\"",
    "version:switch": "node scripts/version-manager.js switch",
    "version:status": "node scripts/version-manager.js status",
    "version:help": "node scripts/version-manager.js help"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@eslint/migrate-config": "^1.5.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/eslint-config": "^8.2.0",
    "@sveltejs/kit": "^2.21.3",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/upgrade": "^4.1.8",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "autoprefixer": "^10.4.16",
    "comlink": "^4.4.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-svelte": "^3.9.1",
    "globals": "^16.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.77.4",
    "svelte": "^5.33.18",
    "svelte-canvas": "^2.0.2",
    "svelte-check": "^4.2.1",
    "svelte-eslint-parser": "^1.2.0",
    "svelte-preprocess": "^6.0.3",
    "svelte-tiny-virtual-list": "^2.0.6",
    "tailwindcss": "^4.1.8",
    "tslib": "^2.6.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "type": "module",
  "packageManager": "pnpm@10.12.0",
  "pnpm": {
    "onlyBuiltDependencies": []
  }
}

{
  "name": "frontend",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite dev",
    "dev:legacy": "cross-env SVELTE_MODE=legacy pnpm install && vite dev",
    "dev:modern": "cross-env SVELTE_MODE=modern pnpm install && vite dev",
    "dev:svelte4": "pnpm run dev:legacy",
    "dev:svelte5": "pnpm run dev:modern",
    "build": "vite build",
    "build:legacy": "cross-env SVELTE_MODE=legacy pnpm install && vite build",
    "build:modern": "cross-env SVELTE_MODE=modern pnpm install && vite build",
    "build:svelte4": "pnpm run build:legacy", 
    "build:svelte5": "pnpm run build:modern",
    "install:legacy": "cross-env SVELTE_MODE=legacy pnpm install",
    "install:modern": "cross-env SVELTE_MODE=modern pnpm install",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.Modern.json",
    "check:legacy": "svelte-check --tsconfig ./tsconfig.Legacy.json",
    "check:modern": "svelte-check --tsconfig ./tsconfig.Modern.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.Modern.json --watch",
    "lint": "prettier --check . && eslint . --no-warn-ignored",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:svelte": "stylelint \"src/**/*.{css,svelte}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,svelte}\" --fix",
    "format": "prettier --write .",
    "test:version": "node -e \"console.log('Svelte version:', require('svelte/package.json').version)\"",
    "mode:status": "node -e \"console.log('Current mode:', process.env.SVELTE_MODE || 'modern (default)')\"",
    "version:switch": "node scripts/version-manager.js switch",
    "version:status": "node scripts/version-manager.js status",
    "version:help": "node scripts/version-manager.js help"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@eslint/migrate-config": "^1.5.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/eslint-config": "^8.2.0",
    "@sveltejs/kit": "^2.21.3",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "autoprefixer": "^10.4.16",
    "comlink": "^4.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-svelte": "^3.9.1",
    "globals": "^16.2.0",
    "postcss": "^8.4.38",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.77.4",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "svelte": "^5.33.18",
    "svelte-canvas": "^2.0.2",
    "svelte-check": "^4.2.1",
    "svelte-eslint-parser": "^1.2.0",
    "svelte-preprocess": "^6.0.3",
    "svelte-tiny-virtual-list": "^2.0.6",
    "tailwindcss": "^4.1.8",
    "timeless-jewels-legacy-config": "file:.config-deps/timeless-jewels-legacy-config/timeless-jewels-legacy-config-1.0.0.tgz",
    "timeless-jewels-modern-config": "file:.config-deps/timeless-jewels-modern-config/timeless-jewels-modern-config-1.0.0.tgz",
    "tslib": "^2.6.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "type": "module",
  "packageManager": "pnpm@10.12.0",
  "pnpm": {
    "onlyBuiltDependencies": []
  }
}
